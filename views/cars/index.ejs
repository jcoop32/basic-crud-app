<%- include('../partials/header') %>

<h2>Cars:</h2>

<table>
    <thead>
        <tr>
            <th>Make</th>
            <th>Model</th>
            <th>Year</th>
            <th>Color</th>
            <!-- <th>Edit</th>
            <th>Delete</th> -->
   
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0; i < cars.length; i++){%>
            <tr>
                <td><%= cars[i].make %></td>
                <td><%= cars[i].model %></td>
                <td><%= cars[i].releaseYear %></td>
                <td><%= cars[i].color %></td>
                <td>
                    <a href="/cars/<%=cars[i]._id%>/edit">
                        <input type="image" src="/images/pencil-square.svg"></input>                    </a>
                </td>
                <td>
                    <form action="/cars/<%=cars[i]._id%>?_method=DELETE" method='POST'>
                           <input type="image" src="/images/trash3 (1).svg"></input>
                    </form>
                </td>
            </tr>
        <%} %>

    </tbody>
</table>









<%- include('../partials/footer') %>